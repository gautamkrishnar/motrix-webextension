{
  "description": "WebExtension for Motrix download manager",
  "manifest_version": 2,
  "name": "Motrix WebExtension",
  "version": "2.0.0",
  "homepage_url": "https://github.com/gautamkrishnar/motrix-chrome-extension/",
  "icons": {
    "16": "assets/images/16.png",
    "32": "assets/images/32.png",
    "48": "assets/images/48.png",
    "128": "assets/images/128.png"
  },
  "permissions": [
    "downloads",
    "storage",
    "notifications"
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "assets/images/16.png",
      "32": "assets/images/32.png",
      "48": "assets/images/48.png",
      "128": "assets/images/128.png"
    },
    "default_title": "Motrix WebExtension",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "r.gautamkrishna@gmail.com",
      "strict_min_version": "69.0"
    }
  }
}